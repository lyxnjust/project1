#include <iostream>
using namespace std;
int prime (int);
int main()
{
    int n,x;
    for(int i=0;i<=n;i++)
    {
        cin>>x;
        if(prime(x)) cout<<x<<" is prime";
        else cout<<x<<" is not prime";
    }
int prime(int x);
{
    int a;
    for(int i=1;i<=x;i++)
    {
        if(!(x%i))
        a++;
        else continue;
    }
    if (a==2) return 1;
    else return 0;
}
    return 0;
}
